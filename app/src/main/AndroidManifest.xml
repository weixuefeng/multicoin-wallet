<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.explorer.wallettest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:name=".App"
        android:theme="@style/Theme.WalletTest">
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.wallet.WalletListActivity"/>
        <activity android:name=".ui.wallet.CreateWalletActivity"/>
        <activity android:name=".ui.wallet.CurrentWalletActivity"/>
        <activity android:name=".ui.wallet.asset.AssetDetailActivity"/>
        <activity android:name=".ui.wallet.transfer.TransferAssetActivity"/>
    </application>

</manifest>